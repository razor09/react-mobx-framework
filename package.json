{
  "scripts": {
    "start": "webpack serve --config webpack/config.ts --mode development",
    "build": "webpack --config webpack/config.ts --mode production",
    "mocks": "npm start -- --name mocks",
    "preview": "ts-node webpack/preview.ts",
    "generate": "ts-node tools/codegen/generate.ts",
    "lint": "prettier --write .",
    "fetch": "rm -rf node_modules && npm ci",
    "update": "ncu -u && rm -rf node_modules package-lock.json && npm i"
  },
  "prettier": {
    "printWidth": 130,
    "semi": false,
    "singleQuote": true,
    "singleAttributePerLine": true
  },
  "simple-git-hooks": {
    "pre-commit": "npm run build && npm run lint && git add --all"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "mobx": "6.15.0",
    "mobx-react-lite": "4.1.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-router-dom": "7.13.1",
    "react-transition-group": "4.4.5"
  },
  "devDependencies": {
    "@faker-js/faker": "10.3.0",
    "@types/http-server": "0.12.4",
    "@types/node": "25.3.1",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@types/react-transition-group": "4.4.12",
    "css-loader": "7.1.4",
    "css-minimizer-webpack-plugin": "7.0.4",
    "html-webpack-plugin": "5.6.6",
    "http-server": "14.1.1",
    "mini-css-extract-plugin": "2.10.0",
    "msw": "2.12.10",
    "npm-check-updates": "19.5.0",
    "prettier": "3.8.1",
    "sass": "1.97.3",
    "sass-loader": "16.0.7",
    "simple-git-hooks": "2.13.1",
    "style-loader": "4.0.0",
    "terser-webpack-plugin": "5.3.16",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "webpack": "5.105.2",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.3"
  }
}
